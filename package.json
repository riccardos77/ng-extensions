{
  "name": "ng-ext",
  "version": "0.0.0",
  "scripts": {
    "publish-core": "ng build @ng-ext/core && pushd dist\\ng-ext\\core && npm pack && popd",
    "publish-forms": "ng build @ng-ext/forms && pushd dist\\ng-ext\\forms && npm pack && popd",
    "publish-router": "ng build @ng-ext/router && pushd dist\\ng-ext\\router && npm pack && popd",
    "ng": "ng",
    "start": "ng serve --port 4290",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.1.0",
    "@angular/common": "~13.1.0",
